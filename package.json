{
  "name": "webpaack-template",
  "version": "0.9.0",
  "description": "webpack-template for project creation",
  "main": "index.html",
  "scripts": {
    "dev": "npm run dev-build& npm run dev-server",
    "dev-build": "webpack --env=dev --progress --colors --config webpack/webpack.config.js",
    "dev-server": "webpack-dev-server --env=dev --progress --colors --config webpack/webpack.config.js",
    "beta-build": "webpack --env=beta --progress --colors --config webpack/webpack.config.js",
    "real-build": "webpack --env=prod --progress --colors --config webpack/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webpaack-template"
  ],
