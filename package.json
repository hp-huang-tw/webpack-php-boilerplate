    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "compass-mixins": "^0.12.10",
    "css-loader": "^0.28.2",
    "enumify": "^1.0.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-replace-webpack-plugin": "^2.2.4",
    "html-webpack-plugin": "^2.28.0",
    "ip": "^1.1.5",
    "node-sass": "^4.5.3",
